{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/main.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_key",
    "HUBSPOT_TOKEN": "@hubspot_token",
    "SLACK_BOT_TOKEN": "@slack_bot_token",
    "SLACK_CHANNEL": "@slack_channel",
    "GOOGLE_CREDENTIALS_FILE": "@google_creds"
  }
}
